params.index_path = "$projectDir/work/index/"
params.index_basename = "mm39"
params.input_dir = "$projectDir/work/fastq/move"
params.gtf_file = "$projectDir/work/gencode.vM10.annotation.gtf"
params.pairedEnd = true
params.temp_dir = "$projectDir/work/tmp"
// Enable Singularity. Set default docker image
process {
    container = 'nfcore/rnaseq:1.4.2'
}

params {
max_cpus = 1
max_memory = 8.GB
max_time = 1.h
}

docker.enabled = true
